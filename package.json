{
  "name": "oracle-games",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3210",
    "dev:staging": "dotenv -e .env.staging -- next dev --turbopack --port 3210",
    "dev:emulator": "dotenv -e .env.emulator -- next dev --turbopack --port 3210",
    "prod": "next dev --turbopack --port 3210",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --quiet",
    "emulators": "export PATH=\"/opt/homebrew/opt/openjdk@17/bin:$PATH\" && firebase emulators:start --import=./emulator-data --export-on-exit",
    "emulators:clear": "rm -rf ./emulator-data && export PATH=\"/opt/homebrew/opt/openjdk@17/bin:$PATH\" && firebase emulators:start --export-on-exit=./emulator-data",
    "emulators:seed": "export PATH=\"/opt/homebrew/opt/openjdk@17/bin:$PATH\" && node scripts/seed-emulator.js",
    "seed:test-data": "tsx scripts/seed-test-data.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:full": "start-server-and-test emulators http://127.0.0.1:4000 'start-server-and-test dev:emulator http://localhost:3210 scripts/run-tests-with-seed.sh'",
    "test:e2e:full:ui": "start-server-and-test emulators http://127.0.0.1:4000 'start-server-and-test dev:emulator http://localhost:3210 \"scripts/run-tests-with-seed.sh --ui\"'"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.20",
    "@floating-ui/dom": "^1.7.4",
    "@motiadev/stream-client-react": "^0.8.4-beta.141",
    "@tiptap/core": "^3.12.1",
    "@tiptap/extension-link": "^3.11.1",
    "@tiptap/extension-underline": "^3.11.1",
    "@tiptap/pm": "^3.12.1",
    "@tiptap/react": "^3.11.1",
    "@tiptap/starter-kit": "^3.11.1",
    "@uidotdev/usehooks": "^2.4.1",
    "@vercel/speed-insights": "^1.3.1",
    "ai": "^5.0.87",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "firebase": "^12.5.0",
    "firebase-admin": "^13.6.0",
    "firestore": "^1.1.6",
    "flag-icons": "^7.5.0",
    "framer-motion": "^12.23.24",
    "i18next": "^25.7.2",
    "i18next-icu": "^2.4.1",
    "mailersend": "^2.6.0",
    "next": "15.5.7",
    "openai": "^6.8.0",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "19.2.1",
    "react-currency-input-field": "^4.0.3",
    "react-datepicker": "^8.10.0",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.4.1",
    "react-intersection-observer": "^10.0.0",
    "react-range-slider-input": "^3.3.2",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.1",
    "tabler-icons-react": "^1.56.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "istanbul-lib-coverage": "^3.2.2",
    "nyc": "^17.1.0",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^4",
    "ts-loader": "^9.5.4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}
